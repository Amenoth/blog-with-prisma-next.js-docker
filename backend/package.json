{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "",
  "scripts": {
    "start": "npm -s run build && node dist/server",
    "clean": "rimraf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "migrations:generate": "dotenv -e migrate.env -- prisma migrate save --experimental --create-db",
    "migrations:push": "dotenv -e migrate.env -- prisma migrate up --experimental --create-db",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server",
    "postinstall": "npm -s run generate"
  },
  "dependencies": {
    "@nexus/schema": "^0.16.0",
    "@prisma/cli": "^2.9.0",
    "@prisma/client": "^2.9.0",
    "@types/node-fetch": "^2.5.7",
    "apollo-server": "^2.18.2",
    "graphql": "^15.3.0",
    "nexus-plugin-prisma": "^0.22.0"
  },
  "devDependencies": {
    "@types/node": "^14.11.10",
    "dotenv-cli": "^4.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "1.0.0-pre.50",
    "typescript": "^4.0.3"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  }
}
